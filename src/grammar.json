{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "chariot",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "recipe_ref"
          },
          {
            "type": "SYMBOL",
            "name": "object"
          }
        ]
      }
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "//"
        },
        {
          "type": "PATTERN",
          "value": "(\\\\+(.|\\r?\\n)|[^\\\\\\n])*"
        }
      ]
    },
    "tok_identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][_\\.\\-a-zA-Z0-9]*"
    },
    "tok_string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "tok_code_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[a-zA-Z]*"
          },
          "named": true,
          "value": "lang"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "code"
            }
          },
          "named": true,
          "value": "content"
        },
        {
          "type": "SYMBOL",
          "name": "close_tag"
        }
      ]
    },
    "fragment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "object"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "recipe_ref"
        },
        {
          "type": "SYMBOL",
          "name": "code_block"
        }
      ]
    },
    "string": {
      "type": "SYMBOL",
      "name": "tok_string"
    },
    "code_block": {
      "type": "SYMBOL",
      "name": "tok_code_block"
    },
    "recipe_ref": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "*"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "tok_identifier"
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "SYMBOL",
          "name": "tok_identifier"
        }
      ]
    },
    "object": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "tok_identifier"
              },
              {
                "type": "STRING",
                "value": ":"
              },
              {
                "type": "SYMBOL",
                "name": "fragment"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "fragment"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "close_tag"
    },
    {
      "type": "SYMBOL",
      "name": "code"
    },
    {
      "type": "SYMBOL",
      "name": "error_sentinel"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}