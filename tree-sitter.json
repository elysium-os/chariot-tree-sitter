{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "chariot",
      "camelcase": "Chariot",
      "title": "Chariot",
      "scope": "source.chariot",
      "file-types": [
        "chariot"
      ],
      "injection-regex": "^chariot$",
      "class-name": "TreeSitterChariot"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Chariot DSL Tree Sitter",
    "authors": [
      {
        "name": "WuX"
      }
    ],
    "links": {
      "repository": "https://github.com/elysium-os/chariot"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
